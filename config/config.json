{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "business-application-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/businessApplication/BusinessApplicationWebPart.js",
          "manifest": "./src/webparts/businessApplication/BusinessApplicationWebPart.manifest.json"
        }
      ]
    },
    "alertwebpart-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/alertwebpart/AlertwebpartWebPart.js",
          "manifest": "./src/webparts/alertwebpart/AlertwebpartWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "BusinessApplicationWebPartStrings": "lib/webparts/businessApplication/loc/{locale}.js",
    "AlertwebpartWebPartStrings": "lib/webparts/alertwebpart/loc/{locale}.js"
  }
}
